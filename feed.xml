---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="{{ site.lang }}">
    <title>{{ site.title | xml_escape }} - Articles</title>
    <subtitle>Latest entries in Blog {{ site.author }}â€™s Blog</subtitle>
    <author>
        <name>{{ site.author }}</name>
    </author>
    <link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/atom+xml"/>
    <link href="{{ site.baseurl | prepend: site.url }}" rel="alternate" type="text/html" hreflang="{{ site.lang }}"/>
    <id>{{ page.url | absolute_url | xml_escape }}</id>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <generator uri="https://jekyllrb.com/" version="{{ jekyll.version }}">Jekyll</generator>
    <icon>{{ site.favicon | prepend: site.baseurl | prepend: site.url }}</icon>
    {% for post in site.posts limit:10 %}
    {% unless post.draft %}
    <entry>
        <title>{{ post.title | xml_escape }}</title>
        <link href="{{ post.url | absolute_url }}" rel="alternate" type="text/html" title="{{ post.title | xml_escape }}" />
        <id>{{ post.id | absolute_url | xml_escape }}</id>
        <published>{{ post.date | date_to_xmlschema }}</published>
        <updated>{{ post.last_modified_at | default: post.date | date_to_xmlschema }}</updated>
        <summary>{{ post.excerpt | strip_html | xml_escape }}</summary>
        {% for category in post.categories %}{% if category != 'blog' %}
        <category term="{{ category | xml_escape }}"/>
        {% endif %}{% endfor %}
    </entry>
    {% endunless %}
    {% endfor %}
</feed>
