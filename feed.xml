---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns:atom="http://www.w3.org/2005/Atom" xmlns:dc="http://purl.org/dc/elements/1.1/">
    <channel>
        <title>{{ site.title | xml_escape }} - Articles</title>
        <subtitle>Latest entries in Blog {{ site.author }}â€™s Blog</subtitle>
        <author>
            <name>{{ site.author }}</name>
        </author>
        <link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self"/>
        <link href="{{ site.baseurl | prepend: site.url }}"/>
        <id>{{ site.baseurl | prepend: site.url }}</id>
        <updated>{{ site.time | date_to_rfc822 }}</updated>
        <generator>Jekyll v{{ jekyll.version }}</generator>
        <icon>{{ site.favicon | prepend: site.baseurl | prepend: site.url }}</icon>
        {% for post in site.posts limit:10 %}
        {% unless post.draft %}
        <entry>
            <title>{{ post.title | xml_escape }}</title>
            <link href="{{ post.url | prepend: site.baseurl | prepend: site.url }}"/>
            <id>{{ post.url | prepend: site.baseurl | prepend: site.url }}</id>
            <updated>{{ post.date | date_to_rfc822 }}</updated>
            <summary>{{ post.excerpt | strip_html | xml_escape }}</summary>
            {% for category in post.categories %}{% if category != 'blog' %}
            <category term="{{ category | xml_escape }}"/>
            {% endif %}{% endfor %}
        </entry>
        {% endunless %}
        {% endfor %}
    </channel>
</feed>
